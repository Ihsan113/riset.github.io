{
  "text": "<!DOCTYPE html>\n<html lang=\"en\">\n  <head>\n    <meta charset=\"utf-8\" />\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\n    <title>Company Profile - [Your Company Name]</title>\n    \n    <!-- Muat Tailwind CSS dari CDN -->\n    <script src=\"https://cdn.tailwindcss.com\"></script>\n    <!-- Konfigurasi Tailwind -->\n    <script>\n      tailwind.config = {\n        darkMode: 'class',\n        theme: {\n          extend: {\n            colors: {\n              primary: '#1E40AF',\n              secondary: '#64748B'\n            }\n          }\n        }\n      };\n    </script>\n    \n    <!-- Font Awesome & Google Fonts -->\n    <link href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css\" rel=\"stylesheet\" />\n    <link href=\"https://fonts.googleapis.com/css2?family=Roboto:wght@400;700&display=swap\" rel=\"stylesheet\" />\n    \n    <style>\n      /* Gaya umum */\n      body { font-family: 'Roboto', sans-serif; transition: background-color 0.3s, color 0.3s; }\n      .hamburger { cursor: pointer; transition: all 0.3s; }\n      /* Mobile menu */\n      .mobile-menu { animation: slideDown 0.3s ease-out; }\n      @keyframes slideDown { from { opacity: 0; transform: translateY(-20px); } to { opacity: 1; transform: translateY(0); } }\n      /* Tombol dark mode */\n      .theme-switcher { position: fixed; bottom: 2rem; right: 2rem; z-index: 50; padding: 0.75rem; border-radius: 9999px; background-color: #fff; box-shadow: 0 4px 6px rgba(0,0,0,0.1); transition: background-color 0.3s, transform 0.3s; }\n      .theme-switcher:hover { transform: scale(1.05); }\n      /* Overlay efek mesin ketik */\n      #overlay { position: fixed; top: 0; left: 0; width: 100%; height: 100%; background: #000; color: #0f0; display: flex; justify-content: center; align-items: center; z-index: 1000; padding: 1rem; }\n      #typewriter { font-family: monospace; font-size: 1rem; white-space: pre-wrap; }\n      /* Efek fade-out & fade-in */\n      .fade-out { opacity: 0; transition: opacity 1s ease; }\n      .fade-in { opacity: 0; transition: opacity 1s ease; }\n      .fade-in.visible { opacity: 1; }\n    </style>\n  </head>\n  <body class=\"bg-gray-100 dark:bg-gray-900 text-gray-900 dark:text-gray-100\">\n    \n    <!-- Overlay: Efek Mesin Ketik -->\n    <div id=\"overlay\">\n      <pre id=\"typewriter\"></pre>\n    </div>\n    \n    <!-- Tombol Dark Mode -->\n    <button id=\"theme-switcher\" class=\"theme-switcher bg-white dark:bg-gray-800 text-gray-900 dark:text-gray-100\">\n      <i id=\"theme-icon\" class=\"fas fa-moon\"></i>\n    </button>\n    \n    <!-- Header (disembunyikan sampai overlay selesai) -->\n    <header id=\"header\" class=\"fade-in bg-white dark:bg-gray-800 shadow fixed w-full top-0 z-40\">\n      <div class=\"container mx-auto px-4 py-4 flex justify-between items-center\">\n        <div class=\"text-2xl font-bold\">[Your Company Name]</div>\n        <!-- Desktop Navigation -->\n        <nav class=\"hidden md:flex space-x-6\">\n          <a class=\"hover:text-blue-600 dark:hover:text-blue-400 transition-colors\" href=\"#\">Home</a>\n          <a class=\"hover:text-blue-600 dark:hover:text-blue-400 transition-colors\" href=\"#companies\">Our Companies</a>\n          <a class=\"hover:text-blue-600 dark:hover:text-blue-400 transition-colors\" href=\"#services\">Services</a>\n          <a class=\"hover:text-blue-600 dark:hover:text-blue-400 transition-colors\" href=\"#about\">About</a>\n          <a class=\"hover:text-blue-600 dark:hover:text-blue-400 transition-colors\" href=\"#contact\">Contact</a>\n        </nav>\n        <!-- Tombol Hamburger Mobile -->\n        <div class=\"md:hidden hamburger\">\n          <i class=\"fas fa-bars text-xl\"></i>\n        </div>\n      </div>\n      <!-- Mobile Menu -->\n      <div id=\"mobile-menu\" class=\"mobile-menu md:hidden bg-white dark:bg-gray-800 hidden\">\n        <div class=\"container mx-auto px-4 py-4 flex flex-col space-y-4\">\n          <a class=\"py-2 hover:text-blue-600 dark:hover:text-blue-400\" href=\"#\">Home</a>\n          <a class=\"py-2 hover:text-blue-600 dark:hover:text-blue-400\" href=\"#companies\">Our Companies</a>\n          <a class=\"py-2 hover:text-blue-600 dark:hover:text-blue-400\" href=\"#services\">Services</a>\n          <a class=\"py-2 hover:text-blue-600 dark:hover:text-blue-400\" href=\"#about\">About</a>\n          <a class=\"py-2 hover:text-blue-600 dark:hover:text-blue-400\" href=\"#contact\">Contact</a>\n        </div>\n      </div>\n    </header>\n    \n    <!-- Main Content (disembunyikan sampai overlay selesai) -->\n    <main id=\"main\" class=\"fade-in container mx-auto px-4 pt-24 pb-10\">\n      <!-- Hero Section -->\n      <section class=\"text-center mb-16\">\n        <h1 class=\"text-4xl md:text-5xl font-bold mb-6\">\n          Welcome to [Your Company Name]\n        </h1>\n        <p class=\"text-lg md:text-xl text-gray-600 dark:text-gray-300 max-w-2xl mx-auto\">\n          Innovative solutions for web, mobile, and bot development. Experience the fusion of classic business values with modern technology.\n        </p>\n      </section>\n      \n      <!-- Our Companies Section -->\n      <section id=\"companies\" class=\"mb-16\">\n        <h2 class=\"text-3xl font-bold text-center mb-8\">Our Companies</h2>\n        <div class=\"grid grid-cols-1 md:grid-cols-2 gap-8\">\n          <!-- ForestAPI Card -->\n          <div class=\"bg-white dark:bg-gray-800 p-6 rounded-xl shadow-lg hover:shadow-xl transition-shadow text-center\">\n            <img alt=\"Logo of ForestAPI\" class=\"mx-auto mb-4 w-24 h-24 object-contain rounded-full\" src=\"https://storage.googleapis.com/a1aa/image/cwkkmaSEoWX7EakcaIcMd4QaFw4x65H12J6LptD9MTY.jpg\" width=\"100\" height=\"100\" />\n            <h3 class=\"text-xl font-bold mb-2\">ForestAPI</h3>\n            <p class=\"text-gray-600 dark:text-gray-300\">\n              Your gateway to seamless API integrations and robust backend solutions.\n            </p>\n          </div>\n          <!-- UniShop Card -->\n          <div class=\"bg-white dark:bg-gray-800 p-6 rounded-xl shadow-lg hover:shadow-xl transition-shadow text-center\">\n            <img alt=\"Logo of UniShop\" class=\"mx-auto mb-4 w-24 h-24 object-contain rounded-full\" src=\"https://storage.googleapis.com/a1aa/image/C6C9Rsm7e2lH3V7_giHeyzOBRiDHH9bZG5DKTKoIkNY.jpg\" width=\"100\" height=\"100\" />\n            <h3 class=\"text-xl font-bold mb-2\">UniShop</h3>\n            <p class=\"text-gray-600 dark:text-gray-300\">\n              Empowering e-commerce with innovative shopping solutions and seamless integrations.\n            </p>\n          </div>\n        </div>\n      </section>\n      \n      <!-- Services Section -->\n      <section id=\"services\" class=\"mb-16\">\n        <h2 class=\"text-3xl font-bold text-center mb-8\">Our Services</h2>\n        <div class=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-8\">\n          <!-- Web Development Service -->\n          <div class=\"bg-white dark:bg-gray-800 p-6 rounded-xl shadow-lg hover:shadow-xl transition-shadow text-center\">\n            <i class=\"fas fa-code text-4xl mb-4 text-blue-600\"></i>\n            <h3 class=\"text-xl font-bold mb-2\">Web Development</h3>\n            <p class=\"text-gray-600 dark:text-gray-300\">\n              Building responsive websites with modern design and optimal performance.\n            </p>\n          </div>\n          <!-- Mobile App Development -->\n          <div class=\"bg-white dark:bg-gray-800 p-6 rounded-xl shadow-lg hover:shadow-xl transition-shadow text-center\">\n            <i class=\"fas fa-mobile-alt text-4xl mb-4 text-blue-600\"></i>\n            <h3 class=\"text-xl font-bold mb-2\">Mobile Applications</h3>\n            <p class=\"text-gray-600 dark:text-gray-300\">\n              Creating user-friendly mobile apps for both Android and iOS platforms.\n            </p>\n          </div>\n          <!-- WhatsApp Bot -->\n          <div class=\"bg-white dark:bg-gray-800 p-6 rounded-xl shadow-lg hover:shadow-xl transition-shadow text-center\">\n            <i class=\"fab fa-whatsapp text-4xl mb-4 text-green-500\"></i>\n            <h3 class=\"text-xl font-bold mb-2\">WhatsApp Bot</h3>\n            <p class=\"text-gray-600 dark:text-gray-300\">\n              Automating customer interactions and support via WhatsApp.\n            </p>\n          </div>\n          <!-- Telegram Bot -->\n          <div class=\"bg-white dark:bg-gray-800 p-6 rounded-xl shadow-lg hover:shadow-xl transition-shadow text-center\">\n            <i class=\"fab fa-telegram-plane text-4xl mb-4 text-blue-500\"></i>\n            <h3 class=\"text-xl font-bold mb-2\">Telegram Bot</h3>\n            <p class=\"text-gray-600 dark:text-gray-300\">\n              Delivering fast and reliable solutions through Telegram automation.\n            </p>\n          </div>\n        </div>\n      </section>\n      \n      <!-- About Section -->\n      <section id=\"about\" class=\"text-center mb-16\">\n        <div class=\"max-w-4xl mx-auto\">\n          <h2 class=\"text-3xl font-bold mb-6\">About Us</h2>\n          <p class=\"text-lg text-gray-600 dark:text-gray-300 mb-8\">\n            We are a dynamic company dedicated to transforming your digital presence with cutting-edge technology and creative solutions. Our portfolio spans web development, mobile apps, and bot automation.\n          </p>\n          <img alt=\"Team Photo\" class=\"mx-auto rounded-xl shadow-lg w-full h-auto\" src=\"https://storage.googleapis.com/a1aa/image/AqcKVhCAhJ5b6QsBEdra1GW8n2QtPC2aGLN0S6q4cFA.jpg\" width=\"800\" height=\"400\" />\n        </div>\n      </section>\n      \n      <!-- Contact Section -->\n      <section id=\"contact\" class=\"mb-16\">\n        <h2 class=\"text-3xl font-bold text-center mb-8\">Get in Touch</h2>\n        <div class=\"max-w-2xl mx-auto\">\n          <form class=\"bg-white dark:bg-gray-800 p-6 rounded-xl shadow-lg\">\n            <div class=\"mb-4\">\n              <label class=\"block text-gray-700 dark:text-gray-200 mb-2\" for=\"name\">Name</label>\n              <input class=\"w-full p-3 border rounded focus:outline-none focus:ring-2 focus:ring-blue-600\" id=\"name\" type=\"text\" placeholder=\"Your name\">\n            </div>\n            <div class=\"mb-4\">\n              <label class=\"block text-gray-700 dark:text-gray-200 mb-2\" for=\"email\">Email</label>\n              <input class=\"w-full p-3 border rounded focus:outline-none focus:ring-2 focus:ring-blue-600\" id=\"email\" type=\"email\" placeholder=\"Your email\">\n            </div>\n            <div class=\"mb-4\">\n              <label class=\"block text-gray-700 dark:text-gray-200 mb-2\" for=\"message\">Message</label>\n              <textarea class=\"w-full p-3 border rounded focus:outline-none focus:ring-2 focus:ring-blue-600\" id=\"message\" rows=\"5\" placeholder=\"Your message\"></textarea>\n            </div>\n            <button class=\"w-full bg-blue-600 text-white p-3 rounded hover:bg-blue-700 transition-colors\" type=\"submit\">\n              Send Message\n            </button>\n          </form>\n        </div>\n      </section>\n    </main>\n    \n    <!-- Footer (disembunyikan sampai overlay selesai) -->\n    <footer id=\"footer\" class=\"fade-in bg-white dark:bg-gray-800 py-8 border-t border-gray-200 dark:border-gray-700\">\n      <div class=\"container mx-auto px-4 text-center\">\n        <div class=\"flex flex-col md:flex-row justify-between items-center mb-6\">\n          <div class=\"mb-4 md:mb-0\">\n            <div class=\"text-xl font-bold mb-2\">[Your Company Name]</div>\n            <p class=\"text-sm text-gray-600 dark:text-gray-300\">Innovating since 2023</p>\n          </div>\n          <div class=\"flex space-x-6\">\n            <a class=\"text-gray-600 dark:text-gray-300 hover:text-blue-600 dark:hover:text-blue-400\" href=\"#\">\n              <i class=\"fab fa-facebook-f\"></i>\n            </a>\n            <a class=\"text-gray-600 dark:text-gray-300 hover:text-blue-600 dark:hover:text-blue-400\" href=\"#\">\n              <i class=\"fab fa-twitter\"></i>\n            </a>\n            <a class=\"text-gray-600 dark:text-gray-300 hover:text-blue-600 dark:hover:text-blue-400\" href=\"#\">\n              <i class=\"fab fa-linkedin-in\"></i>\n            </a>\n          </div>\n        </div>\n        <p class=\"text-sm text-gray-600 dark:text-gray-300\">\n          Â© 2023 [Your Company Name]. All rights reserved.\n        </p>\n      </div>\n    </footer>\n    \n    <!-- JavaScript -->\n    <script>\n      // Fungsi mesin ketik\n      function startTypewriter(codeText) {\n        const typewriterEl = document.getElementById('typewriter');\n        let index = 0;\n        function type() {\n          if (index < codeText.length) {\n            typewriterEl.textContent += codeText.charAt(index);\n            index++;\n            setTimeout(type, 10);\n          } else {\n            setTimeout(() => {\n              const overlay = document.getElementById('overlay');\n              overlay.classList.add('fade-out');\n              setTimeout(() => {\n                overlay.remove();\n                document.getElementById('header').classList.add('visible');\n                setTimeout(() => {\n                  document.getElementById('main').classList.add('visible');\n                  setTimeout(() => {\n                    document.getElementById('footer').classList.add('visible');\n                  }, 500);\n                }, 500);\n              }, 1000);\n            }, 500);\n          }\n        }\n        type();\n      }\n      \n      // Ambil data JSON dari file code.json\n      fetch('code.json')\n        .then(response => response.json())\n        .then(data => {\n          startTypewriter(data.text);\n        })\n        .catch(err => console.error('Error mengambil kode dari JSON:', err));\n      \n      // Dark Mode Toggle\n      const themeButton = document.getElementById('theme-switcher');\n      const themeIcon = document.getElementById('theme-icon');\n      function toggleTheme() {\n        const isDark = document.documentElement.classList.toggle('dark');\n        themeIcon.className = isDark ? 'fas fa-sun' : 'fas fa-moon';\n        localStorage.setItem('theme', isDark ? 'dark' : 'light');\n      }\n      if (\n        localStorage.getItem('theme') === 'dark' ||\n        (!localStorage.getItem('theme') && window.matchMedia('(prefers-color-scheme: dark)').matches)\n      ) {\n        document.documentElement.classList.add('dark');\n        themeIcon.className = 'fas fa-sun';\n      }\n      themeButton.addEventListener('click', toggleTheme);\n      \n      // Hamburger Menu Toggle (Mobile)\n      const hamburger = document.querySelector('.hamburger');\n      const mobileMenu = document.getElementById('mobile-menu');\n      hamburger.addEventListener('click', (e) => {\n        e.stopPropagation();\n        mobileMenu.classList.toggle('hidden');\n      });\n      document.addEventListener('click', (e) => {\n        if (!hamburger.contains(e.target) && !mobileMenu.contains(e.target)) {\n          mobileMenu.classList.add('hidden');\n        }\n      });\n    <\\/script>\n  </body>\n</html>"
}
